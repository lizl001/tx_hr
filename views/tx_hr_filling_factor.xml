<odoo>
    <data>
        <record id="view_filling_factor_tree" model="ir.ui.view">
            <field name="name">tx_hr_filling_factor.tree</field>
            <field name="model">tx.hr.filling.factor</field>
            <field name="arch" type="xml">
                <tree string="补卡系数" editable="top" create="0">
                    <field name="create_uid" readonly="1"/>
                    <field name="create_date" readonly="1"/>
                    <field name="write_date" readonly="1"/>
                    <field name="write_uid" readonly="1"/>
                    <field name="filling_factor"/>
                </tree>
            </field>
        </record>

        <record id="view_filling_factor" model="ir.actions.act_window">
            <field name="name">补卡系数</field>
            <field name="res_model">tx.hr.filling.factor</field>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>
