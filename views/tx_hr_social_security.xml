<odoo>
    <data>
        <record id="tx_hr_social_security_tree" model="ir.ui.view">
            <field name="name">tx.hr.social.security.tree</field>
            <field name="model">tx.hr.social.security</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                    <field name="money"/>
                </tree>
            </field>
        </record>
        <record id="tx_hr_social_security" model="ir.actions.act_window">
            <field name="name">社保金额类型</field>
            <field name="res_model">tx.hr.social.security</field>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>