<odoo>
    <data>
        <record id="view_overtime_rate_tree" model="ir.ui.view">
            <field name="name">tx.hr.overtime.rate.tree</field>
            <field name="model">tx.hr.overtime.rate</field>
            <field name="arch" type="xml">
                <tree string="加班工资比例" editable="top" create="0">
                    <field name="create_uid" readonly="1"/>
                    <field name="create_date" readonly="1"/>
                    <field name="write_date" readonly="1"/>
                    <field name="write_uid" readonly="1"/>
                    <field name="regular_overtime_pay"/>
                    <field name="holiday_overtime_pay"/>
                </tree>
            </field>
        </record>

        <record id="view_overtime_rate" model="ir.actions.act_window">
            <field name="name">加班工资比例</field>
            <field name="res_model">tx.hr.overtime.rate</field>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>
